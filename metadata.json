{
    "name": "4chan X - YouTube Playlist",
    "version": "2.3.5",
    "namespace": "4chan-X-yt-playlist",
    "description": "Wraps all YouTube videos inside a thread into a playlist",
    "include": [
        "https://boards.4chan.org/*/thread/*",
        "https://warosu.org/*/thread/*"
    ],
    "grant": [
        "GM_setValue",
        "GM_getValue"
    ],
    "run-at": "document-start"
}